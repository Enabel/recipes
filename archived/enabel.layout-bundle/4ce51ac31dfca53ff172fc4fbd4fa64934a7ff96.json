                    "assets/": "assets/",
                    "public/": "public/bundles/enabellayout/"
                },
                "copy-from-recipe": {
                    "config/": "%CONFIG_DIR%/",
                    "templates/": "templates/"
                },
                "add-lines": [
                    {
                        "file": "assets/app.js",
                        "content": "/** Remove content above this line */\nimport './bootstrap.js';\nimport '@enabel/enabel-bootstrap-theme/dist/css/enabel-bootstrap-theme.min.css'\nimport '@enabel/enabel-bootstrap-theme/dist/css/variables.min.css'\nimport '@enabel/enabel-bootstrap-theme/dist/css/error.min.css'\nimport '@enabel/enabel-bootstrap-theme/dist/js/enabel-bootstrap-theme.min'\nimport '@fontsource-variable/maven-pro/index.min.css';\nimport './styles/app.scss';\n",
                        "position": "bottom"
                    }
                ],
                "post-install-output": [
                    "  * Your layout is almost ready:",
                    "    0. If you are not using Symfony CLI, replace symfony console with bin/console in the following commands.",
                    "",
                    "    1. Install the dependencies with AssetMapper by running the following commands:",
                    "        <comment>symfony console importmap:require @enabel/enabel-bootstrap-theme</comment>",
                    "        <comment>symfony console importmap:require @enabel/enabel-bootstrap-theme/dist/css/enabel-bootstrap-theme.min.css</comment>",
                    "        <comment>symfony console importmap:require @enabel/enabel-bootstrap-theme/dist/css/variables.min.css</comment>",
