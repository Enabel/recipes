{
    "manifests": {
        "enabel/user-bundle": {
            "manifest": {
                "bundles": {
                    "Enabel\\UserBundle\\EnabelUserBundle": [
                        "all"
                    ],
                    "KnpU\\OAuth2ClientBundle\\KnpUOAuth2ClientBundle": [
                        "all"
                    ]
                },
                "container": {
                    "locale": "en",
                    "enabled_locales": [
                        "en",
                        "fr"
                    ]
                },
                "copy-from-recipe": {
                    "config/": "%CONFIG_DIR%/",
                    "templates/": "templates/",
                    "src/": "%SRC_DIR%/"
                },
                "env": {
                    "AZURE_CLIENT_ID": "paste_here_the_client_id",
                    "AZURE_CLIENT_SECRET": "paste_here_the_secret_id"
                },
                "add-lines": [
                    {
                        "file": "config/packages/knpu_oauth2_client.yaml",
                        "content": "        azure_o365:\n            type: azure\n            client_id: '%env(AZURE_CLIENT_ID)%'\n            client_secret: '%env(AZURE_CLIENT_SECRET)%'\n            api_version: 'v1.0'\n            url_api: 'https://graph.microsoft.com/'\n            redirect_route: enabel_azure_check\n",
                        "position": "after_target",
                        "target": "clients:",
                        "requires": "knpuniversity/oauth2-client-bundle"
                    },
                    {
                        "file": "config/packages/stof_doctrine_extensions.yaml",
                        "content": "    orm:\n        default:\n            timestampable: true",
                        "position": "bottom",
                        "requires": "stof/doctrine-extensions-bundle",
                        "warn_if_missing": true
                    }
                ]
            },
            "files": {
                "config/packages/enabel_user.yaml": {
                    "contents": [
                        "enabel_user:",
                        "    login_redirect_route: 'app_home'",
                        "    user_class: 'App\\Entity\\Enabel\\User'",
                        "    user_repository: 'App\\Repository\\Enabel\\UserRepository'",
                        ""
                    ],
                    "executable": false
                },
                "config/routes/user.yaml": {
                    "contents": [
                        "enabel_user:",
                        "    resource: \"@EnabelUserBundle/config/routes.yaml\"",
                        ""
                    ],
                    "executable": false
                },
                "src/Controller/Admin/Enabel/UserCrudController.php": {
                    "contents": [
                        "<?php",
                        "",
                        "declare(strict_types=1);",
                        "",
                        "namespace App\\Controller\\Admin\\Enabel;",
                        "",
                        "use App\\Entity\\Enabel\\User;",
                        "use Enabel\\UserBundle\\Controller\\Admin\\UserCrudController as BaseUserCrudController;",
                        "",
                        "class UserCrudController extends BaseUserCrudController",
                        "{",
                        "    public static function getEntityFqcn(): string",
                        "    {",
                        "        return User::class;",
                        "    }",
                        "}",
                        ""
                    ],
                    "executable": false
                },
                "src/Entity/.gitignore": {
                    "contents": [
                        ""
                    ],
                    "executable": false
                },
                "src/Entity/Enabel/User.php": {
                    "contents": [
                        "<?php",
                        "",
                        "declare(strict_types=1);",
                        "",
                        "namespace App\\Entity\\Enabel;",
                        "",
                        "use App\\Repository\\Enabel\\UserRepository;",
                        "use Doctrine\\ORM\\Mapping as ORM;",
                        "use Enabel\\UserBundle\\Entity\\User as BaseUser;",
                        "",
                        "#[ORM\\Entity(repositoryClass: UserRepository::class)]",
                        "class User extends BaseUser",
                        "{",
                        "}",
                        ""
                    ],
                    "executable": false
                },
                "src/Repository/.gitignore": {
                    "contents": [
                        ""
                    ],
                    "executable": false
                },
                "src/Repository/Enabel/UserRepository.php": {
                    "contents": [
                        "<?php",
                        "",
                        "declare(strict_types=1);",
                        "",
                        "namespace App\\Repository\\Enabel;",
                        "",
                        "use App\\Entity\\Enabel\\User;",
                        "use Doctrine\\Persistence\\ManagerRegistry;",
                        "use Enabel\\UserBundle\\Repository\\UserRepository as BaseUserRepository;",
                        "",
                        "/**",
                        " * @method User|null   find($id, $lockMode = null, $lockVersion = null)",
                        " * @method User|null   findOneBy(array $criteria, array $orderBy = null)",
                        " * @method array<User> findAll()",
                        " * @method array<User> findBy(array $criteria, array $orderBy = null, $limit = null, $offset = null)",
                        " */",
                        "class UserRepository extends BaseUserRepository",
                        "{",
                        "    public function __construct(ManagerRegistry $registry)",
                        "    {",
                        "        parent::__construct($registry, User::class);",
                        "    }",
                        "}",
                        ""
                    ],
                    "executable": false
                }
            },
            "ref": "d06dcddb752214ebf83bfe24065b61ecbc854204"
        }
    }
}
