{
    "manifests": {
        "enabel/coding-standard-bundle": {
            "manifest": {
                "bundles": {
                    "Enabel\\CodingStandardBundle\\EnabelCodingStandardBundle": [
                        "all"
                    ]
                },
                "copy-from-package": {
                    "assets/docker/": "docker/",
                    "assets/phpstan/": "phpstan/",
                    "assets/.phpmd.xml": ".phpmd.xml",
                    "assets/.stylelintignore": ".stylelintignore",
                    "assets/.stylelintrc": ".stylelintrc",
                    "assets/docker-compose.yml": "docker-compose.yml",
                    "assets/docker-compose.override.yml": "docker-compose.override.yml",
                    "assets/Makefile": "Makefile",
                    "assets/php.ini": "php.ini",
                    "assets/phpinsights.php": "phpinsights.php",
                    "assets/phpstan.neon": "phpstan.neon",
                    "assets/rector.php": "rector.php"
                },
                "env": {
                    "NODE_VERSION": "16",
                    "MYSQL_VERSION": "8",
                    "PHP_VERSION": "8.1",
                    "DATABASE_URL": "mysql://app:!ChangeMe!@127.0.0.1:3306/app?serverVersion=${MYSQL_VERSION}&charset=utf8mb4"
                },
                "gitignore": [
                    "/%PUBLIC_DIR%/coverage/",
                    "/symfony",
                    "/__symfony_bak__",
                    "!/docker/db/data/.gitignore",
                    "!/docker/redis/data/.gitignore",
                    "!/docker/redis/insight/.gitignore"
                ],
                "post-install-output": [
                    "  * Modify your DATABASE_URL config in <fg=green>.env</>",
                    "",
                    "  * Install the JavaScript dependencies by running <comment>yarn add stylelint stylelint-config-prettier stylelint-config-twbs-bootstrap --dev</comment>",
                    "",
                    "  * Edit <comment>webpack.config.js</comment> and <info>uncomment/add</info> the following lines:",
                    "",
                    "            Encore",
                    "                // ...",
                    "                .addEntry('enabel', './assets/enabel.js')",
                    "                .enableSassLoader()",
                    "",
                    "  * Build assets by running <comment>make assets</comment>"
                ]
            },
            "files": [],
            "ref": "2807c1ff449f739f2f313bd67231bc38c433fe51"
        }
    }
}
